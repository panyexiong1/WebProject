<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shu.patient.domain">
    <class name="Patient" table="patient">
        <id name="pid">
            <generator class="uuid"></generator>
        </id>

        <property name="ppid"></property>
        <property name="pname"></property>
        <property name="sex"></property>
        <property name="birthday"></property>
        <property name="xueli"></property>
        <property name="datasource"></property>

        <set name="patientBingli" cascade="delete" inverse="true">
            <key column="pid"></key>
            <one-to-many class="PatientBingli"></one-to-many>
        </set>
        <set name="patientPic" cascade="delete" inverse="true">
            <key column="pid"></key>
            <one-to-many class="PatientPic"></one-to-many>
        </set>


    </class>
</hibernate-mapping>