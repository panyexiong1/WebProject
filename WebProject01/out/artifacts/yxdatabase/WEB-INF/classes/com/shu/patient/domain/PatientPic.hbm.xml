<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shu.patient.domain">
    <class name="PatientPic" table="patient_pic">
        <id name="tid">
            <generator class="uuid"></generator>
        </id>

        <property name="checkdate"></property>
        <property name="sjlx1"></property>
        <property name="sjlx2"></property>
        <property name="clqsj"></property>
        <property name="clhsj"></property>
        <property name="pssb"></property>

        <!--多对一：多条图片信息对应一位病人信息-->
        <many-to-one name="patient" class="Patient" column="pid"></many-to-one>

    </class>
</hibernate-mapping>