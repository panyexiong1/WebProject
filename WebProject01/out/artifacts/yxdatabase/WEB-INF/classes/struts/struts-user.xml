<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="user" namespace="/" extends="common" strict-method-invocation="false">
        <action name="userAction_*" class="com.shu.user.web.action.UserAction" method="{1}">
            <result name="success">/WEB-INF/views/home.jsp</result>
            <result name="login">/WEB-INF/views/login.jsp</result>
        </action>
    </package>
</struts>