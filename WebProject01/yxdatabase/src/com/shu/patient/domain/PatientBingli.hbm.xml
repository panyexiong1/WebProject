<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shu.patient.domain">
    <class name="PatientBingli" table="patient_bingli">
        <id name="bid">
            <generator class="uuid"></generator>
        </id>

        <property name="checkdate"></property>
        <property name="height"></property>
        <property name="weight"></property>
        <property name="xuetang"></property>
        <property name="jblx"></property>
        <property name="qingbingsj"></property>
        <property name="qingbingzt"></property>
        <property name="bingcheng"></property>
        <property name="DM"></property>
        <property name="UPDRS"></property>
        <property name="HY"></property>
        <property name="MMSE"></property>
        <property name="MOCA"></property>
        <property name="CDRSB"></property>
        <property name="ADAS11"></property>
        <property name="ADAS13"></property>
        <property name="RAVLT_immediate"></property>
        <property name="RAVLT_learning"></property>
        <property name="RAVLT_forgetting"></property>

        <many-to-one name="patient" class="Patient" column="pid"></many-to-one>


    </class>
</hibernate-mapping>